#include <stdio.h>

int main ()
{
    int n1, n2, n3, aux;

    printf("Escreva o primeiro numero: ");
    scanf("%d", &n1);
    printf("Escreva o segundo numero: ");
    scanf("%d", &n2);
    printf("Escreva o terceiro numero: ");
    scanf("%d", &n3);

    if (n1 > n2 && n1 >n3)
    {
        printf("\nO maior numero eh %d\n", n1);
    }

    if (n2 > n3 && n2 > n1)
    {
        printf("\nO maior numero eh %d\n", n2);
    }
    if (n3 > n2 && n3 > n1)
    {
        printf("\nO maior numero eh %d\n", n3);
    }
    return 0;
}
