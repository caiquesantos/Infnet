#include <stdio.h>

main()
{
    int n1, n2;

    printf("Entre com o primeiro numero: ");
    scanf("%d", &n1);
    printf("Entre com o segundo numero: ");
    scanf("%d", &n2);

    if (n1 > n2)
        printf("\nO primeiro numero eh maior que o segundo\n");
    else
        printf("\nO segundo numero eh maior que o primeiro\n");
}
