#include <stdio.h>

int main ()
{
    int n1, n2, n3, aux;

    printf("Escreva o primeiro numero: ");
    scanf("%d", &n1);
    printf("Escreva o segundo numero: ");
    scanf("%d", &n2);
    printf("Escreva o terceiro numero: ");
    scanf("%d", &n3);

    if (n1 > n2)
    {
        aux = n1;
        n1 = n2;
        n2 = aux;

    }

    if (n1 > n3)
    {
        aux = n1;
        n1 = n3;
        n3 = aux;

    }
    if (n2 > n3)
    {
        aux = n2;
        n2 = n3;
        n3 = aux;
    }

    printf("\nA sequencia sera %d, %d, %d\n", n1, n2, n3);
    return 0;
}

